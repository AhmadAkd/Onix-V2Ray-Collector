# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_CHANNELS=@v2rayngvpn,@freev2ray,@vpnconfigs,@proxynetwork,@v2raycollect

# Discord Bot Configuration (Optional)
DISCORD_BOT_TOKEN=your_discord_token_here
DISCORD_CHANNELS=1234567890,0987654321

# Reddit API Configuration (Optional)
REDDIT_CLIENT_ID=your_client_id
REDDIT_CLIENT_SECRET=your_client_secret
REDDIT_USER_AGENT=V2RayCollector/1.0

# GitHub Personal Access Token (Optional)
GITHUB_TOKEN=your_github_token_here

# Advanced Testing
ENABLE_ADVANCED_TESTING=true
TEST_TIMEOUT=10
MAX_CONCURRENT_TESTS=200

# Monitoring
ENABLE_MONITORING=true
MONITORING_INTERVAL=300  # seconds
DATABASE_PATH=monitoring.db

# Alerts
ENABLE_ALERTS=true
ALERT_EMAIL=your_email@example.com
ALERT_WEBHOOK=https://hooks.slack.com/services/YOUR/WEBHOOK/URL

# Performance
MAX_WORKERS=200
CACHE_TTL=1800  # seconds
BATCH_SIZE=500

# Protocol Support
SUPPORTED_PROTOCOLS=vmess,vless,trojan,ss,ssr,hysteria,hy2,hysteria2,hysteria3,tuic,tuic5,naive,reality,wireguard

# Sources
MAX_SOURCES=100
SOURCE_TIMEOUT=30

# Categorization
MAX_CONFIGS_PER_PROTOCOL=2000
MAX_CONFIGS_PER_COUNTRY=1000
MIN_CONFIGS_PER_PROTOCOL=10

# Quality Filters
MIN_LATENCY=5000  # ms
SUCCESS_RATE_THRESHOLD=0.7
UPTIME_THRESHOLD=0.8
